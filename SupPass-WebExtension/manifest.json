{
  "manifest_version": 2,
  "name": "SupPass",
  "version": "2.0",
  "author": "Kevin Delbegue",
  "description": "SupPass -- No Credidentials Storage. No Problem.",
  "homepage_url": "https://github.com/keviiin38/SupPass",
  "icons": {
    "32": "img/icons/SupPass-padlock-32.png",
    "48": "img/icons/SupPass-padlock-48.png",
    "64": "img/icons/SupPass-padlock-64.png",
    "128": "img/icons/SupPass-padlock-128.png",
    "256": "img/icons/SupPass-padlock-256.png",
    "512": "img/icons/SupPass-padlock-512.png",
    "640": "img/icons/SupPass-padlock-640.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "img/icons/SupPass-padlock-32.png",
    "default_title": "SupPass",
    "default_popup": "popup/SupPass.html"
  }
}